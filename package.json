{
  "name": "@jianger666/vue-to-react-mcp",
  "version": "1.0.1",
  "description": "MCP server for Vue to React migration automation",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "vue-to-react-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepare": "npm run build",
    "test": "node --test",
    "prepublishOnly": "npm run build && npm test",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "mcp",
    "vue",
    "react",
    "migration",
    "automation"
  ],
  "author": "Chen Lujiang",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "@types/fs-extra": "^11.0.4"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "diff": "^5.1.0",
    "simple-git": "^3.22.0",
    "winston": "^3.11.0",
    "fs-extra": "^11.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jianger666/vue-to-react-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/jianger666/vue-to-react-mcp/issues"
  },
  "homepage": "https://github.com/jianger666/vue-to-react-mcp#readme"
}
